      <div class="page-header">          
        <h1>New Agent</h1>
      </div>

      <div class="col-md-3" id="leftCol">
        <div class="well">    
          <h3>Single</h3>   
          <%= render 'layouts/sidebar_show' %>
        </div>
      </div>     

      <div class="col-md-9" id="rightCol"> 
        <div class="well">          
          <div class="panel panel-info">
            <div class="panel-body"> 

<p>
