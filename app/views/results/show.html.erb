<div class="bs-docs-section">
  <div class="row">
    <div class="col-md-12">


      <div class="page-header">          
        <h1>
          Concentration Result <%= @result.id %>
        </h1>
      </div>

      <div class="col-md-3" id="leftCol">
        <div class="well">    
          <h3>Single</h3>   
          <%= render 'layouts/sidebar_show' %>
        </div>
      </div>     

      <div class="col-md-9" id="rightCol"> 
        <div class="well">          
          <div class="panel panel-info">
            <div class="panel-body"> 


              <p>
                <strong>Cfu initial:</strong>
                <%= @result.cfu_initial %>
              </p>

              <p>
                <strong>Cfu initial unit:</strong>
                <%= @result.cfu_initial_unit %>
              </p>

              <p>
                <strong>Detectability:</strong>
                <%= @result.detectability %>
              </p>

              <p>
                <strong>Cfu detected:</strong>
                <%= @result.cfu_detected %>
              </p>

              <p>
                <strong>Cfu detected unit:</strong>
                <%= @result.cfu_detected_unit %>
              </p>

              <p>
                <strong>R comment:</strong>
                <%= @result.r_comment %>
              </p>


            </div>
          </div>    
          <%= link_to 'Edit', edit_result_path(@result) %> |
          <%= link_to 'Back', results_path %>   
        </div>
      </div>

    </div>     
  </div>
</div>
